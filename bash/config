export CLICOLOR=1
export LSCOLORS=gxfxcxdxbxegedabagacad
export HISTCONTROL=ignoredups

export PS1='\[\033[01;32m\]\n[\u@\h]\[\033[00m\] \[\033[01;36m\]\w \[\033[01;35m\]$(__git_ps1 "(%s)")\n\[\033[01;32m\][$(rbenv version-name)] [\t] \[\033[01;36m\]$\[\033[00m\] '

if [ -f `/usr/local/bin/brew --prefix`/etc/bash_completion ]; then
  source `/usr/local/bin/brew --prefix`/etc/bash_completion
fi

complete -o default -o nospace -F _git_checkout gco
complete -C ~/bin/cap_completion -o default cap
