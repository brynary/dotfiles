[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[user]
  name = Bryan Helmkamp
  email = bryan@brynary.com
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[github]
  user = brynary
[core]
  autocrlf = input
  safecrlf = true
  excludesfile = /Users/bhelmkamp/.gitignore
[push]
  default = simple
